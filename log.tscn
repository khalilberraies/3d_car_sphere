script/source = "extends Node2D

onready var epsilon_input = $EpsilonInput
onready var alpha_input = $AlphaInput
onready var gamma_input = $GammaInput
onready var start_training_button = $StartTrainingButton
onready var start_training_button2 = $StartTrainingButton2
onready var title_label = $TitleLabel

func _ready():
	start_training_button.connect(\"pressed\", self, \"_on_StartTrainingButton_pressed\")
	start_training_button2.connect(\"pressed\", self, \"_on_StartTrainingButton2_pressed\")
	customize_interface()

func customize_interface():
	start_training_button.text = \"Choisir le scénario 1\"
	start_training_button.rect_min_size = Vector2(200, 50)
	start_training_button.add_color_override(\"bg_color\", Color(0.2, 0.6, 0.2))
	start_training_button.add_color_override(\"font_color\", Color(1, 1, 1))

	start_training_button2.text = \"Choisir le scénario 2\"
	start_training_button2.rect_min_size = Vector2(200, 50)
	start_training_button2.add_color_override(\"bg_color\", Color(0.2, 0.2, 0.6))
	start_training_button2.add_color_override(\"font_color\", Color(1, 1, 1))

func _on_StartTrainingButton_pressed():
	get_tree().change_scene(\"res://testscene.tscn\")

func _on_StartTrainingButton2_pressed():
	get_tree().change_scene(\"res://testscen.tscn\")
"

